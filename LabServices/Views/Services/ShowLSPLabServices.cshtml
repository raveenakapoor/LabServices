@model LabServices.Models.AddLabService

@{
    ViewBag.Title = "ShowLSPLabServices";
}

<h2></h2>

@using (Html.BeginForm())
{
    <div class="form-horizontal">

        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">

            <div class="col-md-10">
                @Html.TextBox("myTextBox")
                
            </div>
            <div class="col-md-2">
                <span id="lbluserid">jkhjg</span>
            </div>
        </div>
        </div>

        <table class="table">

            <tr>

                <th>
                    Test Code
                </th>
                <th>
                    Lab ID
                </th>
                <th>
                    Test Name
                </th>
                <th>
                    Test Description
                </th>
                <th>
                    Cost of Test
                </th>
                <th>
                    Lab Home
                </th>
                <th>
                    Test Duration
                </th>


            </tr>

            @for (int i = 0; i < @Model.Data.Tables[0].Rows.Count; i++)
            {
                <tr>

                    <td>
                        @Model.Data.Tables[0].Rows[i]["TestCode"].ToString()
                    </td>
                    <td>
                        @Model.Data.Tables[0].Rows[i]["LabId"].ToString()
                    </td>
                    <td>
                        @Model.Data.Tables[0].Rows[i]["TestName"].ToString()
                    </td>
                    <td>
                        @Model.Data.Tables[0].Rows[i]["TestDescription"].ToString()
                    </td>
                    <td>
                        @Model.Data.Tables[0].Rows[i]["CostOfTest"].ToString()
                    </td>
                    <td>
                        @Model.Data.Tables[0].Rows[i]["LabHome"].ToString()
                    </td>
                    <td>
                        @Model.Data.Tables[0].Rows[i]["TestDuration"].ToString()
                    </td>

                </tr>
            }

        </table>
        }



